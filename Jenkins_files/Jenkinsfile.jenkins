pipeline {
    agent any
    stages {
        stage('Clone feature branche') {
			when {
				not {
					branch 'master'
				}
			}
            steps {
                sh 'export BRANCH=$(git branch | cut -d " " -f2)'
				sh 'git clone --branch=$BRANCH https://github.com/Threadripper1/Large-pipe.git'
            }
        }
		stage('List Docker files') {
			when {
				not {
					branch 'master'
				}
			}
			steps {
				sh 'find . Dockerfile & cat Dockerfile'
				sh 'pwd'
			}
		}
	}
}
